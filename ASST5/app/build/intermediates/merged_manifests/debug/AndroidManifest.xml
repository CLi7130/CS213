<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asst5"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="27"
        android:targetSdkVersion="32" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="android.support.v4.app.CoreComponentFactory"
        android:debuggable="true"
        android:extractNativeLibs="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.ASST5" >
        <activity
            android:name="com.example.asst5.StoreOrdersActivity"
            android:label="@string/store_orders_title"
            android:parentActivityName="com.example.asst5.MainActivity" />
        <activity
            android:name="com.example.asst5.CurrentOrderActivity"
            android:label="@string/current_order_title"
            android:parentActivityName="com.example.asst5.MainActivity" />
        <activity
            android:name="com.example.asst5.OrderCoffeeActivity"
            android:label="@string/order_coffee_title"
            android:parentActivityName="com.example.asst5.MainActivity" />
        <activity
            android:name="com.example.asst5.OrderDonutActivity"
            android:label="@string/order_donuts_title"
            android:parentActivityName="com.example.asst5.MainActivity" />
        <activity
            android:name="com.example.asst5.DonutQuantityActivity"
            android:label="@string/donut_quantity_title"
            android:parentActivityName="com.example.asst5.OrderDonutActivity" />
        <activity
            android:name="com.example.asst5.MainActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>